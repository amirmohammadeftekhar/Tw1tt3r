<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<VBox alignment="TOP_CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="350.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.ChoosingMenuController">
   <children>
      <VBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="500.0" styleClass="primary-color" stylesheets="@../css/style.css" VBox.vgrow="ALWAYS">
               <content>
                  <GridPane fx:id="gridPane" alignment="TOP_CENTER" styleClass="primary-color" stylesheets="@../css/style.css">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="73.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="doneButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#doneButtonAction" stylesheets="@../css/style.css" text="Done">
               <styleClass>
                  <String fx:value="mouse-moved" />
                  <String fx:value="rect-round" />
                  <String fx:value="shadow" />
               </styleClass>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
